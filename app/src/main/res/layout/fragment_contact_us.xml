<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="sonu.kumar.examscorer.Fragments.ContactUsFragment">
    <android.support.v4.widget.NestedScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">
    <RelativeLayout
        android:layout_margin="10dp"
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <ImageView
            android:id="@+id/my_profile_pic"
            android:layout_width="match_parent"
            android:layout_height="170dp"
            android:layout_margin="10dp"
            android:scaleType="centerCrop"
            android:src="@drawable/sonu_profile"
            />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="PHONE"
            android:textSize="18sp"
            android:padding="5dp"
            android:id="@+id/phone_title"
            android:layout_marginTop="5dp"
            android:layout_marginLeft="5dp"
            android:layout_marginRight="5dp"
            android:layout_below="@+id/my_profile_pic"
            android:fontFamily="@font/custom_font_dark"
            android:textStyle="bold"/>
        <View
            android:id="@+id/phone_under_line"
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="#af000000"
           android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            android:layout_below="@+id/phone_title"/>
        <TextView
            android:textStyle="bold"
            android:fontFamily="@font/custom_font"
            android:textColor="#000"
            android:id="@+id/phone_num1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="8283941295"
            android:textSize="18sp"
            android:layout_margin="10dp"
            android:layout_below="@+id/phone_under_line"/>

        <ImageView
            android:id="@+id/message_phone1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBottom="@+id/phone_num1"
            android:layout_alignEnd="@+id/phone_under_line"
            android:layout_alignRight="@+id/phone_under_line"
            android:src="@drawable/ic_call_black_24dp" />

        <TextView
            android:textStyle="bold"
            android:fontFamily="@font/custom_font"
            android:textColor="#000"
            android:id="@+id/phone_num2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="8557816595"
            android:textSize="18sp"
            android:layout_margin="10dp"
            android:layout_below="@+id/phone_num1"/>

        <ImageView
            android:id="@+id/message_phone2"
            android:layout_below="@+id/message_phone1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignTop="@+id/phone_num2"
            android:layout_alignRight="@+id/phone_under_line"
            android:src="@drawable/ic_call_black_24dp" />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="EMAIL"
            android:textSize="18sp"
            android:padding="5dp"
            android:id="@+id/email_title"
            android:layout_marginTop="5dp"
            android:layout_marginLeft="5dp"
            android:layout_marginRight="5dp"
            android:layout_below="@+id/phone_num2"
            android:fontFamily="@font/custom_font_dark"
            android:textStyle="bold"/>
        <View
            android:id="@+id/email_title_line"
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="#af000000"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            android:layout_below="@+id/email_title"/>

        <TextView
            android:textStyle="bold"
            android:fontFamily="@font/custom_font"
            android:textColor="#000"
            android:id="@+id/email_addres_my"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="skr3704@gmail.com"
            android:textSize="18sp"
            android:layout_margin="10dp"
            android:layout_below="@+id/email_title"/>

        <ImageView
            android:id="@+id/email_icon"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBottom="@+id/email_addres_my"
            android:layout_alignEnd="@+id/email_title_line"
            android:layout_alignRight="@+id/email_title_line"
            android:src="@drawable/ic_email_black_24dp" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="WEBSITE"
            android:textSize="18sp"
            android:padding="5dp"
            android:id="@+id/website_title"
            android:layout_marginTop="5dp"
            android:layout_marginLeft="5dp"
            android:layout_marginRight="5dp"
            android:layout_below="@+id/email_addres_my"
            android:fontFamily="@font/custom_font_dark"
            android:textStyle="bold"/>

        <View
            android:id="@+id/wesite_line"
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="#af000000"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            android:layout_below="@+id/website_title"/>
        <TextView
            android:textStyle="bold"
            android:fontFamily="@font/custom_font"
            android:textColor="#000"
            android:id="@+id/wesite_link"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="examscorer.co.in"
            android:textSize="18sp"
            android:layout_margin="10dp"
            android:layout_below="@+id/website_title"/>

        <ImageView
            android:id="@+id/wesite_visite"
            android:layout_alignTop="@+id/wesite_link"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_alignBottom="@+id/wesite_link"
            android:layout_alignEnd="@+id/wesite_line"
            android:layout_alignRight="@+id/wesite_line"
            android:src="@drawable/ic_open_in_browser_black_24dp" />

    </RelativeLayout>
    </android.support.v4.widget.NestedScrollView>
</FrameLayout>
